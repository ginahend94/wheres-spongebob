{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "quotes": ["error", "single"],
    "linebreak-style": "off",
    "indent": ["error", 2],
    "no-return-assign": ["error", "except-parens"],
    "operator-linebreak":"off",
    "consistent-return":"warn",
    "comma-dangle":"off",
    "no-param-reassign": ["error", {"props": false}]
  }
}